<?xml version="1.0" encoding="UTF-8"?>
<apiConfig>
  <api id="menu" expire="1m">
    <description>메뉴를 정보를 반환한다.</description>
    <parameter>
      <type default="all" desc="구분(main|mega|svc|all)">(main|mega|svc|all)</type>
      <key desc="키(메뉴 or category)">.{1,20}</key>
    </parameter>
    <request type="module">jmnet.moka.core.dps.api.handler.module.MenuModule</request>
    <keys>
      type
      key
    </keys>
  </api>
  <api id="category" expire="1m">
    <description>Category정보를 반환한다.</description>
    <parameter>
      <master desc="마스터코드">[0-9]{7}</master>
      <service desc="서비스코드">[0-9]{4,6}</service>
      <source desc="출처코드">[0-9]{1,3}</source>
    </parameter>
    <request type="module">jmnet.moka.core.dps.api.handler.module.CategoryModule</request>
    <keys>
      master
      service
      source
    </keys>
  </api>
</apiConfig>