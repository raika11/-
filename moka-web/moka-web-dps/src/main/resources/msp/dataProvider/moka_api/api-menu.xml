<?xml version="1.0" encoding="UTF-8"?>
<apiConfig>
  <api id="menu" expire="1m">
    <description>메뉴를 정보를 반환한다.</description>
    <parameter>
      <division default="all">(all|top|sub)</division>
      <key>.{1,20}</key>
    </parameter>
    <request type="module">jmnet.moka.core.dps.api.handler.module.MenuModule</request>
    <keys>
      division
      key
    </keys>
  </api>
  <api id="category" expire="1m">
    <description>Category정보를 반환한다.</description>
    <parameter>
      <master>[0-9]{7}</master>
      <service>[0-9]{4,6}</service>
      <source>[0-9]{1,3}</source>
    </parameter>
    <request type="module">jmnet.moka.core.dps.api.handler.module.CategoryModule</request>
    <keys>
      master
      service
      source
    </keys>
  </api>
</apiConfig>