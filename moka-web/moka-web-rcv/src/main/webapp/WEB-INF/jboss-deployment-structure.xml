<?xml version="1.0" encoding="UTF-8"?>
<jboss-deployment-structure>
  <deployment>
    <exclusions>
      <!-- Disable jboss slf4j module to enable Logback -->
      <module name="org.slf4j" />
    </exclusions>
    <exclude-subsystems>
      <!-- Disable jboss slf4j module to enable Logback -->
      <subsystem name="logging" />
    </exclude-subsystems>
  </deployment>
</jboss-deployment-structure>