<?xml version="1.0" encoding="UTF-8"?>
<apiConfig>
	<api id="list" expire="1m">
		<description>기사 목록을 반환한다.</description>
		<parameter>
			<title desc="기사제목">.{0,1000}</title>
			<type desc="기사타입" default="C">^[CYNO]{1}$</type>
			<count desc="조회 건수" default="20" type="number">^(3|5|10|20|30|100|200|300|[12345][05])$</count>
		</parameter>
		<request>dps.news.list</request>
		<request total="Y">dps.news.list_total</request>
		<keys>
			title
			type
			#paging
		</keys>
	</api>
